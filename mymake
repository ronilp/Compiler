execute : driver.o stack.o lexer.o parser.o n-tree.o ast.o symbolTable.o
	gcc -Wall -Wextra -o execute driver.o stack.o lexer.o parser.o n-tree.o ast.o symbolTable.o

driver.o : driver.c
	gcc -c driver.c

stack.o : stack.c
	gcc -c stack.c

lexer.o : lexer.c
	gcc -c lexer.c

parser.o : parser.c
	gcc -c parser.c
	
n-tree.o : n-tree.c
	gcc -c n-tree.c
	
ast.o : ast.c
	gcc -c ast.c
symbolTable.o : symbolTable.c
	gcc -c symbolTable.c